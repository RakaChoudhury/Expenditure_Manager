<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.expenditure_manager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.expenditure_manager.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.expenditure_manager.MenuActivity"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name="com.example.expenditure_manager.IncomeActivity"
            android:label="@string/title_activity_income" >
        </activity>
        <activity
            android:name="com.example.expenditure_manager.ExpenditureActivity"
            android:label="@string/title_activity_expenditure" >
        </activity>
        <activity
            android:name="com.example.expenditure_manager.IncActivity"
            android:label="@string/title_activity_inc" >
        </activity>
        <activity
            android:name="com.example.expenditure_manager.StatusActivity"
            android:label="@string/title_activity_status" >
        </activity>
        <activity
            android:name="com.example.expenditure_manager.AccountActivity"
            android:label="@string/title_activity_account" >
        </activity>
        <activity
            android:name="com.example.expenditure_manager.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.example.expenditure_manager.GroupActivity"
            android:label="@string/title_activity_group" >
        </activity>
        <activity
            android:name="com.example.expenditure_manager.ShowExpActivity"
            android:label="@string/title_activity_show_exp" >
        </activity>
        <activity
            android:name="com.example.expenditure_manager.AddExpenditureActivity"
            android:label="@string/title_activity_add_expenditure" >
        </activity>
        <activity
            android:name="com.example.expenditure_manager.ExpActivity"
            android:label="@string/title_activity_exp" >
        </activity>
    </application>

</manifest>